<html>

<body>
    <table>
        <tr>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        <% for(var i=0;i<users.length;i++){ %>
            <tr>
                <td>
                    <%= users[i].Email %>
                </td>
                <td>
                    <%= users[i].Firstname %>
                </td>
                <td>
                    <%= users[i].Lastname %>
                </td>
            </tr>
            <% } %>
    </table>
</body>

</html>